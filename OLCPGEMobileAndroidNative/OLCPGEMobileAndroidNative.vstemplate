      <ProjectItem ReplaceParameters="false" TargetFileName="main.cpp">main.cpp</ProjectItem>
      <ProjectItem ReplaceParameters="false" TargetFileName="android_native_app_glue.h">android_native_app_glue.h</ProjectItem>
      <ProjectItem ReplaceParameters="false" TargetFileName="olcPixelGameEngine_Mobile.h">olcPixelGameEngine_Mobile.h</ProjectItem>
      <ProjectItem ReplaceParameters="false" TargetFileName="pch.h">pch.h</ProjectItem>
      <ProjectItem ReplaceParameters="false" TargetFileName="sse2neon.h">sse2neon.h</ProjectItem>
      <ProjectItem ReplaceParameters="false" TargetFileName="stb_image.h">stb_image.h</ProjectItem>
    </Project>
  </TemplateContent>
  <WizardExtension>
    <Assembly>Microsoft.VisualStudio.Android.Project,Version=16.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a</Assembly>
    <FullClassName>Microsoft.VisualStudio.Android.Wizard.NativeTemplateWizard</FullClassName>
  </WizardExtension>
</VSTemplate>