<VSTemplate Version="3.0.0" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005" Type="Project">
  <TemplateData>
    <Name>OLCPGEMobileAndroidNative</Name>
    <Description>OLC Pixel Game Engine Android Native Activity Release 2.2.1</Description>
    <ProjectType>VC</ProjectType>
    <TemplateID>OLCPGEMobile.2.0.NativeActivity.DLL</TemplateID>
    <SortOrder>1000</SortOrder>
    <CreateNewFolder>true</CreateNewFolder>
    <DefaultName>OLCPGEMobileAndroidNative</DefaultName>
    <ProvideDefaultName>true</ProvideDefaultName>
    <LocationField>Enabled</LocationField>
    <EnableLocationBrowseButton>true</EnableLocationBrowseButton>
    <Icon>CPPPhoneDynamic.ico</Icon>
    <Hidden>true</Hidden>
  </TemplateData>
  <TemplateContent PreferedSolutionConfiguration="Debug|ARM" BuildOnLoad="false">
    <Project TargetFileName="$projectname$.vcxproj" File="OLCPGEMobileAndroidNative.vcxproj" ReplaceParameters="true">
